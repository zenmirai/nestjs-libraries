{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "@zenmirai/nest-storage": [
        "libs/storage/src"
      ],
      "@zenmirai/nest-storage/*": [
        "libs/storage/src/*"
      ],
      "@zenmirai/nest-flip": [
        "libs/flip/src"
      ],
      "@zenmirai/nest-flip/*": [
        "libs/flip/src/*"
      ],
      "@zenmirai/nest-logger": [
        "libs/logger/src"
      ],
      "@zenmirai/nest-logger/*": [
        "libs/logger/src/*"
      ],
      "@zenmirai/nest-notification": [
        "libs/notification/src"
      ],
      "@zenmirai/nest-notification/*": [
        "libs/notification/src/*"
      ]
    }
  }
}