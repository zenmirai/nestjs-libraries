{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "@zenmirai/storage": [
        "libs/storage/src"
      ],
      "@zenmirai/storage/*": [
        "libs/storage/src/*"
      ],
      "flip/flip": [
        "libs/flip/src"
      ],
      "flip/flip/*": [
        "libs/flip/src/*"
      ],
      "@zenmirai/nestjs-logger-module": [
        "libs/logger/src"
      ],
      "@zenmirai/nestjs-logger-module/*": [
        "libs/logger/src/*"
      ]
    }
  }
}