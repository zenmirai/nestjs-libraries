{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@zenmirai/storage/(.*)": "<rootDir>/../libs/storage/src/$1",
    "@zenmirai/storage": "<rootDir>/../libs/storage/src",
    "@zenmirai/nest-flip/(.*)": "<rootDir>/../libs/flip/src/$1",
    "@zenmirai/nest-flip": "<rootDir>/../libs/flip/src",
    "@zenmirai/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@zenmirai/logger": "<rootDir>/../libs/logger/src",
    "@zenmirai/notification/(.*)": "<rootDir>/../libs/notification/src/$1",
    "@zenmirai/notification": "<rootDir>/../libs/notification/src"
  }
}