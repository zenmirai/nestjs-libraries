{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@zenmirai/storage/(.*)": "<rootDir>/../libs/storage/src/$1",
    "@zenmirai/storage": "<rootDir>/../libs/storage/src"
  }
}